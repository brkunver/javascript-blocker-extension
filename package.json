{
  "name": "javascript-blocker-extension",
  "description": "Block user defined javascript on websites",
  "author": "burakhanunver@gmail.com",
  "private": false,
  "version": "2.3.o",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip:all": "git checkout main && concurrently \"bun run zip\" \"bun run zip:firefox\" && git checkout dev",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "manager": "bun manager.cjs bun"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tsconfig/svelte": "^5.0.5",
    "@wxt-dev/module-svelte": "^2.0.3",
    "concurrently": "^9.2.1",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.8"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild",
    "spawn-sync"
  ],
  "dependencies": {
    "@wxt-dev/i18n": "^0.2.4"
  }
}
